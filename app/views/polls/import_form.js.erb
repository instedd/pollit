<%unless @error%>
  $('#poll_form_container').replaceWith('<%=escape_javascript(render :partial => 'form')%>');
  window.reinitialize('#poll_form_container');
  window.formValidate();
  select_field(0);
<%else%>
  window.validator.showErrors({'poll[form_url]': '<%= _("Invalid public Google Form URL.") %>'});
  // reset button
  var import_form_action = $('.import_form_action');
  import_form_action.text('Import');
  import_form_action.removeAttr('disabled');
  import_form_action.removeClass('loading');
<%end%>