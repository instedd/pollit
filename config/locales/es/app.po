msgstr "información estructurada sin la necesidad de utilizar smartphones"

#: app/views/home/index.haml:8
msgid "See and analyze"
msgstr "Visualiza y analiza"

#: app/views/home/index.haml:9
msgid "poll results in real time"
msgstr "los resultados de tu encuesta en tiempo real"

#: app/views/home/index.haml:10
msgid "Conduct"
msgstr "Gestiona"

#: app/views/home/index.haml:11
msgid "polls from anywhere"
msgstr "encuestas desde cualquier lugar"

#: app/views/home/index.haml:12
msgid "Make it easy"
