#formeditor
  padding-top: 20px

  /* ------- General ------------ */

  .nodisplay
    display: none

  /* ------- Redefined Tags ------------ */

  label
    font-size: 14px
    color: #777777
    margin-bottom: 2px
    display: block

  img
    border: 0

  /* ------- Structure ------------ */

  .fieldslist
    padding: 10px 0 10px 0
    float: left
  .fieldslist.large
    width: 100%

  /* ------- Gray Box ------------ */

  .BoxGray
    background: #eeeeee
    -moz-border-radius: 8px
    -webkit-border-radius: 8px
    border-radius: 8px
    -webkit-box-shadow: 2px 2px 3px 0px rgba(0, 0, 0, 0.2)
    -moz-box-shadow: 2px 2px 3px 0px rgba(0, 0, 0, 0.2)
    box-shadow: 2px 2px 3px 0px rgba(0, 0, 0, 0.2)
    font-size: 14px
    padding: 10px 22px 4px 70px
    margin-bottom: 10px
    position: relative
    &.Small
      font-size: 14px
      color: #777777
      padding: 8px 0 10px 10px
    .IconImg
      background: #e1e1e1
      /* old browsers*/
      background: -moz-linear-gradient(top, white 0%, #e1e1e1 100%)
      /* firefox*/
      background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, white), color-stop(100%, #e1e1e1))
      /* webkit*/
      filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffffff', endColorstr='#e1e1e1',GradientType=0 )
      left: 4px
      top: 4px
      height: 55px
      width: 55px
      border: 1px solid #e9e9e9
      -moz-border-radius: 5px
      -webkit-border-radius: 5px
      border-radius: 5px
      position: absolute

  .AddDel
    right: 4px
    top: 6px
    position: absolute

  .Field
    width: 250px
    margin: 0 12px 15px 0
    display: inline-block
    vertical-align: top

  .TextInput
    background: #ffffff
    -webkit-box-shadow: 0px 1px 1px 0px rgba(0, 0, 0, 0.2) inset
    -moz-box-shadow: 0px 1px 1px 0px rgba(0, 0, 0, 0.2) inset
    box-shadow: 0px 1px 1px 0px rgba(0, 0, 0, 0.2) inset
    width: 98%

  .TextInput2
    background: #ffffff
    -webkit-box-shadow: 0px 1px 1px 0px rgba(0, 0, 0, 0.2) inset
    -moz-box-shadow: 0px 1px 1px 0px rgba(0, 0, 0, 0.2) inset
    box-shadow: 0px 1px 1px 0px rgba(0, 0, 0, 0.2) inset
    width: 100%

  .Separator
    margin: 20px 0
    border-bottom: 1px solid #eeeeee
    clear: both

  .Condition
    color: #777777
    line-height: 18px
    margin: 0 0 5px 0
    position: relative

  .AddCondition
    position: absolute
    right: -16px

  .Condition select
    width: 250px
    margin: 0 12px 5px 0

  /* ------- Icons ------------ */

  .RSSl
    background: url(<%= images_path %>icons/rssAuthenticationIcon.png) no-repeat center
    width: 55px
    height: 55px
    display: block

  .RSS
    background: url(<%= images_path %>icons/rssAuthenticationIcon.png) no-repeat center
    width: 55px
    height: 55px
    display: block

  .Filter
    background: url(<%= images_path %>icons/filterIcon.png) no-repeat center
    width: 55px
    height: 55px
    display: block

  .Filterl
    background: url(<%= images_path %>icons/filterAuthenticationIcon.png) no-repeat center
    width: 55px
    height: 55px
    display: block
  /* --------- Widgets ----------- */

  .fieldtop
    &.First
      background: url(<%= images_path %>Back_ArrBox_Top1.png) no-repeat center top
      padding-top: 8px
      width: 620px
      margin-top: 0
    background: url(<%= images_path %>Back_ArrBox_Top2.png) no-repeat center top
    padding-top: 12px
    width: 620px
    margin-top: -8px

  .fieldbot
    background: url(<%= images_path %>Back_ArrBox_Bottom.png) no-repeat center bottom
    width: 620px
    padding-bottom: 22px
    position: relative

  .fieldimg
    background: #e1e1e1
    /* old browsers */
    background: -moz-linear-gradient(top, white 0%, #e1e1e1 100%)
    /* firefox */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, white), color-stop(100%, #e1e1e1))
    /* webkit */
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffffff', endColorstr='#e1e1e1',GradientType=0 )
    left: 4px
    top: 4px
    height: 55px
    width: 55px
    border: 1px solid #e9e9e9
    -moz-border-radius: 5px
    -webkit-border-radius: 5px
    border-radius: 5px
    display: inline-block
    margin: 0 0 0 10px
    vertical-align: top
    &.selected
      background: #d1d1d1
      -webkit-box-shadow: 0px 1px 1px 0px rgba(0, 0, 0, 0.2) inset
      -moz-box-shadow: 0px 1px 1px 0px rgba(0, 0, 0, 0.2) inset
      box-shadow: 0px 1px 1px 0px rgba(0, 0, 0, 0.2) inset

  .fieldmid
    position: relative

  .fielddel
    position: absolute
    right: 8px
    top: -4px
    a
      outline: none
      display: block
      width: 25px
      height: 25px
    .remove_field
      background: url(<%= images_path %>buttons/removeButton.png) no-repeat 0 0
    .remove_field:hover
      background-position-y: -30px
    .remove_field:active
      background-position-y: -60px

  .fieldcontent
    font-size: 12px
    padding: 5px 0 0 4px
    display: inline-block
    vertical-align: top
    .fieldtitle
      font-size: 18px
      color: #000000
      margin-right: 4px
    .fielddesc
      font-size: 12px
      color: #777777
      font-style: italic
      .fieldhint
        padding-right: 20px
    .fieldrequired
      font-style: normal
      img
        position: relative
        top: 2px 
  
  #large
    .addnewtop
      background: url(<%= images_path %>editor/addNewItemBackground_topMarkLarge.png) no-repeat center top
      width: 100%
      position: relative
      left: -20px
    .addnewbot
      background: url(<%= images_path %>editor/addNewItemBackground_bottomLarge.png) no-repeat center bottom
      width: 100%

  .addnewtop
    background: url(<%= images_path %>Back_ArrBox_Top3.png) no-repeat center top
    padding-top: 12px
    width: 620px
    margin-top: -8px

  .addnewbot
    background: url(<%= images_path %>Back_ArrBox_Bottom2.png) no-repeat center bottom
    width: 620px
    height: 120px

  .addnewcontent
    font-size: 10px
    line-height: 10px
    text-align: center
    label
      font-size: 16px
      margin-bottom: 14px
      padding-top: 5px
    a
      color: black
      text-decoration: none  
      outline: none
    

  /* --------- Widgets list -----------*/

  .text
    background: url(<%= images_path %>icons/textIcon.png) no-repeat
    width: 55px
    height: 55px
    display: block

  .number
    background: url(<%= images_path %>icons/NumberIcon.png) no-repeat
    width: 55px
    height: 55px
    display: block

  .date
    background: url(<%= images_path %>icons/dateIcon.png) no-repeat
    width: 55px
    height: 55px
    display: block

  .choose_one
    background: url(<%= images_path %>icons/selectOneIcon.png) no-repeat
    width: 55px
    height: 55px
    display: block

  .select_multiple
    background: url(<%= images_path %>icons/selectMultipleIcon.png) no-repeat
    width: 55px
    height: 55px
    display: block

  .paragraph_text
    background: url(<%= images_path %>icons/paragraphIcon.png) no-repeat
    width: 55px
    height: 55px
    display: block

  .location
    background: url(<%= images_path %>icons/locationIcon.png) no-repeat
    width: 55px
    height: 55px
    display: block

  /* --------- Sidebar -----------*/

  .fieldsidebar
    background: #ffffff
    -webkit-box-shadow: 1px 0px 3px 0px rgba(0, 0, 0, 0.3)
    -moz-box-shadow: 1px 0px 3px 0px rgba(0, 0, 0, 0.3)
    box-shadow: 1px 0px 3px 0px rgba(0, 0, 0, 0.3)
    -moz-border-radius: 5px
    -webkit-border-radius: 5px
    border-radius: 5px
    font-size: 10px
    color: #666666
    margin: 15px 25px 40px 0px
    padding: 10px 10px
    float: right
    width: 240px
    
    p
      margin: 0
      padding: 0
    
    label
      margin: 0
      padding: 0
      font-size: 14px
    label.error
      display: block
      color: #F60
      padding-right: 15px
   
    hr
      padding: 0
      border: none
      border-top: 1px solid #EEE
      margin: 10px 0px 15px 0px
      width: 100%

    .required
      input
        width: auto

    input
      width: 236px
      margin: 3px 0 0 0
      &.small
        width: 176px
        margin: 0
        
    a.addoption
      outline: 0
      display: block
      background-image: url('<%= images_path %>buttons/addSimpleButton.png')
      height: 20px
      width: 20px
      
    a.removeoption
      outline: 0
      display: block
      background-image: url('<%= images_path %>buttons/removeSimpleButton.png')
      height: 20px
      width: 20px

  .fieldslock
    background: url('<%= images_path %>padlockIcon.png') no-repeat 10px 3px
    background-color: #EEE
    padding: 3px 0px 3px 30px
    color: #777777
    margin-top: -10px
    -moz-border-radius: 8px
    -webkit-border-radius: 8px
    border-radius: 8px
  
  .field
    cursor: pointer 

  /* --------- Gral Classes -----------*/
  .left
    float: left

  .right
    float: right

  .clear
    clear: both
